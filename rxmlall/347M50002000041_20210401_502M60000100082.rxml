                  <TableColumn BorderBottom="solid" BorderRight="solid">
                    <Sentence Num="1" WritingMode="vertical">装置又は機器</Sentence>
                  </TableColumn>
                  <TableColumn BorderBottom="solid" BorderRight="solid">
                    <Sentence Num="1" WritingMode="vertical">掲示事項</Sentence>
                  </TableColumn>
                </TableRow>
