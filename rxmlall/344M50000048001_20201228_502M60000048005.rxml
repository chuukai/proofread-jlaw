          <Item Delete="false" Hide="false" Num="10">
            <ItemTitle>十</ItemTitle>
            <ItemSentence>
              <Sentence WritingMode="vertical">第六号に掲げる<span>金額{'事項': 1}</span>を証する書類</Sentence>
            </ItemSentence>
