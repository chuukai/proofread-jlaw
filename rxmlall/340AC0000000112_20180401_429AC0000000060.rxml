    <SupplProvision AmendLawId="408053" AmendLawNum="平成八年五月二九日法律第五三号" Extract="true" Id="b38999d2-1886-11e8-a242-005056b30d52">
      <SupplProvisionLabel>附　則</SupplProvisionLabel>
