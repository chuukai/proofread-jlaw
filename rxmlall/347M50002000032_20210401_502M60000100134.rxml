                <Table WritingMode="vertical">
                  <TableRow>
                    <TableColumn BorderBottom="solid" BorderRight="solid">
                      <Sentence Num="1" WritingMode="vertical">事項</Sentence>
                    </TableColumn>
                    <TableColumn BorderBottom="solid" BorderRight="solid">
                      <Sentence Num="1" WritingMode="vertical">時間</Sentence>
                    </TableColumn>
                  </TableRow>
                  <TableRow>
                    <TableColumn BorderBottom="solid" BorderRight="solid">
                      <Sentence Num="1" WritingMode="vertical">法第六十条第一号に掲げる事項</Sentence>
                      <Sentence Num="2" WritingMode="vertical">一　作業手順の定め方</Sentence>
                      <Sentence Num="3" WritingMode="vertical">二　労働者の適正な配置の方法</Sentence>
                    </TableColumn>
                    <TableColumn BorderBottom="solid" BorderRight="solid">
                      <Sentence Num="1" WritingMode="vertical">二時間</Sentence>
                    </TableColumn>
                  </TableRow>
                  <TableRow>
                    <TableColumn BorderBottom="solid" BorderRight="solid">
                      <Sentence Num="1" WritingMode="vertical">法第六十条第二号に掲げる事項</Sentence>
