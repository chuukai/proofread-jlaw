                    <TableColumn BorderBottom="solid" BorderRight="solid">
                      <Sentence Num="1" WritingMode="vertical">認定事業場</Sentence>
                    </TableColumn>
                    <TableColumn BorderBottom="solid" BorderRight="solid">
                      <Sentence Num="1" WritingMode="vertical">認定に係る店社等</Sentence>
                    </TableColumn>
                  </TableRow>
                </Table>
