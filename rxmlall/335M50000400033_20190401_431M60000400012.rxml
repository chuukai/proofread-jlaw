            <Paragraph Hide="false" Num="1" OldStyle="false">
              <ParagraphNum/>
              <ParagraphSentence>
                <Sentence WritingMode="vertical">前条の規定は、<span>保全{'申請': 1}</span>仮登録について準用する。</Sentence>
