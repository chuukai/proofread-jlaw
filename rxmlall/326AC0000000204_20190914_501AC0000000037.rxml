                <ItemSentence>
                  <Sentence WritingMode="vertical">業務及びその執行に関する事項</Sentence>
                </ItemSentence>
              </Item>
              <Item Delete="false" Hide="false" Num="8">
                <ItemTitle>八</ItemTitle>
                <ItemSentence>
                  <Sentence WritingMode="vertical">会計に関する事項</Sentence>
