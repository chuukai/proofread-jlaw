                      <Subitem1 Num="2">
                        <Subitem1Title>ロ</Subitem1Title>
                        <Subitem1Sentence>
                          <Sentence>特定<span>取締役{'理事': 1}</span>及び特定監査役の間で合意により定めた日があるときは、その日</Sentence>
                        </Subitem1Sentence>
                      </Subitem1>
