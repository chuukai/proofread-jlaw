        <Paragraph Hide="false" Num="1" OldStyle="false">
          <ParagraphNum/>
          <ParagraphSentence>
            <Sentence WritingMode="vertical">法第五条第二項の法務省令で定める<span>事項{'手続': 1}</span>は、<span>次{'規定': 1}</span>のとおりとする。</Sentence>
          </ParagraphSentence>
          <Item Delete="false" Hide="false" Num="1">
            <ItemTitle>一</ItemTitle>
            <ItemSentence>
              <Sentence WritingMode="vertical"><span>登記{'規定': 1}</span><span>情報提供{'手続': 1}</span><span>業務{'規定': 1}</span>の<span>実施{'手続': 1}</span><span>方法{'規定': 1}</span></Sentence>
            </ItemSentence>
          </Item>
          <Item Delete="false" Hide="false" Num="2">
            <ItemTitle>二</ItemTitle>
            <ItemSentence>
              <Sentence WritingMode="vertical"><span>登記{'手続': 1}</span><span>情報提供{'手続': 1}</span><span>業務{'届出': 1}</span>に関する<span>料金{'手続': 1}</span></Sentence>
