                <Column LineBreak="false" Num="1">
                  <Sentence WritingMode="vertical">計算関係書類</Sentence>
                </Column>
                <Column LineBreak="false" Num="2">
                  <Sentence WritingMode="vertical">次に掲げるものをいう。</Sentence>
                </Column>
              </ItemSentence>
              <Subitem1 Delete="false" Hide="false" Num="1">
                <Subitem1Title>イ</Subitem1Title>
                <Subitem1Sentence>
                  <Sentence WritingMode="vertical">成立の日における貸借対照表</Sentence>
                </Subitem1Sentence>
              </Subitem1>
