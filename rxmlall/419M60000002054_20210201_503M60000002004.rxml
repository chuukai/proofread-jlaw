      <Chapter Delete="false" Hide="false" Num="4">
        <ChapterTitle>第四章　雑則</ChapterTitle>
        <Article Delete="false" Hide="false" Num="120">
          <ArticleCaption>（届出書の提出先等）</ArticleCaption>
          <ArticleTitle>第百二十条</ArticleTitle>
          <Paragraph Hide="false" Num="1">
            <ParagraphNum/>
            <ParagraphSentence>
