                  <Subitem1Sentence>
                    <Sentence WritingMode="vertical">三月以上の期間にわたって有効な契約であること。</Sentence>
                  </Subitem1Sentence>
                </Subitem1>
                <Subitem1 Delete="false" Hide="false" Num="3">
