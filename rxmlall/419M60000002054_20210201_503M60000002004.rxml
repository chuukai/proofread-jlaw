          <Article Delete="false" Hide="false" Num="68">
            <ArticleCaption>（取引証拠金等の代用有価証券等）</ArticleCaption>
            <ArticleTitle>第六十八条</ArticleTitle>
            <Paragraph Hide="false" Num="1">
              <ParagraphNum/>
              <ParagraphSentence>
                <Sentence WritingMode="vertical">法第百十九条第五項に規定する内閣府令で定めるものは、次に掲げるものとする。</Sentence>
              </ParagraphSentence>
              <Item Delete="false" Hide="false" Num="1">
                <ItemTitle>一</ItemTitle>
                <ItemSentence>
                  <Sentence WritingMode="vertical">預金契約に基づく債権</Sentence>
