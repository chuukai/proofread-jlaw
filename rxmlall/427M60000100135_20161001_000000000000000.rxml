            <ItemTitle>一</ItemTitle>
            <ItemSentence>
              <Sentence>氏名、生年月日及び住所</Sentence>
            </ItemSentence>
          </Item>
          <Item Num="2">
            <ItemTitle>二</ItemTitle>
            <ItemSentence>
