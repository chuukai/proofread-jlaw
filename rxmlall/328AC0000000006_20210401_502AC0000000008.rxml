        <Article Num="42">
          <ArticleTitle>第四十二条</ArticleTitle>
          <Paragraph Num="1">
            <ParagraphNum/>
            <ParagraphSentence>
              <Sentence>削除</Sentence>
            </ParagraphSentence>
