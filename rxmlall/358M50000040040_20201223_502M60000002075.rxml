            <Subitem1 Delete="false" Hide="false" Num="1">
              <Subitem1Title>イ</Subitem1Title>
              <Subitem1Sentence>
                <Sentence Num="1" WritingMode="vertical">第<span>一条{'内閣府令': 1}</span>の二の二第一項第二号に定める<span>方法{'債務': 1}</span>のうち<span>保証{'受託': 1}</span>等に係る求償権等を<span>譲り受け{'いう': 1}</span>た者が使用するもの</Sentence>
              </Subitem1Sentence>
