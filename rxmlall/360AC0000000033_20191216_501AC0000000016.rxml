        </ParagraphSentence>
      </Paragraph>
    </SupplProvision>
    <SupplProvision AmendLawId="360054" AmendLawNum="昭和六〇年六月七日法律第五四号" Extract="true" Id="8193204a-97a1-46c3-8701-4ef48a56c0d7">
