        <Article Delete="false" Hide="false" Num="8">
          <ArticleCaption>（動産を特定するために必要な事項等）</ArticleCaption>
          <ArticleTitle>第八条</ArticleTitle>
          <Paragraph Hide="false" Num="1">
            <ParagraphNum/>
            <ParagraphSentence>
              <Sentence WritingMode="vertical">法第七条第二項第五号に規定する<span>譲渡{'農業': 1}</span>に係る<span>動産{'規定': 1}</span>を特定するために必要な事項は、次の各号に掲げる<span>区分{'事項': 1, '場合': 1}</span>に従い、当該各号に定める<span>事項{'農業': 1}</span>とする。</Sentence>
            </ParagraphSentence>
            <Item Delete="false" Hide="false" Num="1">
              <ItemTitle>一</ItemTitle>
              <ItemSentence>
                <Sentence WritingMode="vertical">動産の特質によって特定する<span>方法{'農業': 1, '規定': 1}</span></Sentence>
              </ItemSentence>
              <Subitem1 Delete="false" Hide="false" Num="1">
                <Subitem1Title>イ</Subitem1Title>
                <Subitem1Sentence>
                  <Sentence WritingMode="vertical">動産の<span>種類{'農業': 1}</span></Sentence>
                </Subitem1Sentence>
              </Subitem1>
              <Subitem1 Delete="false" Hide="false" Num="2">
                <Subitem1Title>ロ</Subitem1Title>
                <Subitem1Sentence>
                  <Sentence WritingMode="vertical">動産の記号、番号その他の同種類の他の物と識別するために必要な<span>特質{'農業': 1}</span></Sentence>
                </Subitem1Sentence>
              </Subitem1>
            </Item>
            <Item Delete="false" Hide="false" Num="2">
              <ItemTitle>二</ItemTitle>
              <ItemSentence>
                <Sentence WritingMode="vertical">動産の所在によって特定する方法</Sentence>
              </ItemSentence>
              <Subitem1 Delete="false" Hide="false" Num="1">
                <Subitem1Title>イ</Subitem1Title>
                <Subitem1Sentence>
                  <Sentence WritingMode="vertical">動産の<span>種類{'農業': 1}</span></Sentence>
                </Subitem1Sentence>
