              <Column Num="1">
                <Sentence>管理型信託会社及び<span>管理{'当該': 1}</span>型<span>外国{'受託': 1}</span>信託会社</Sentence>
              </Column>
              <Column Num="2">
                <Sentence>千万円</Sentence>
              </Column>
            </ItemSentence>
          </Item>
          <Item Num="3">
            <ItemTitle>三</ItemTitle>
            <ItemSentence>
              <Column Num="1">
                <Sentence>法第五十条の二第一項の登録を受けた者</Sentence>
              </Column>
              <Column Num="2">
                <Sentence>千万円</Sentence>
