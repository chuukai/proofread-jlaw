                  <Subitem2Sentence>
                    <Sentence Function="main" Num="1" WritingMode="vertical">()()に掲げるものを除き、()()</Sentence>
                    <Sentence Function="proviso" Num="2" WritingMode="vertical"></Sentence>
                  </Subitem2Sentence>
                </Subitem2>
                <Subitem2 Delete="false" Hide="false" Num="5">
                  <Subitem2Title>(五)</Subitem2Title>
                  <Subitem2Sentence>
                    <Sentence WritingMode="vertical">()()</Sentence>
                  </Subitem2Sentence>
