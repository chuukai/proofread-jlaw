              <Subitem1 Delete="false" Hide="false" Num="1">
                <Subitem1Title>イ</Subitem1Title>
                <Subitem1Sentence>
                  <Sentence WritingMode="vertical">前号<span>イ{'ロ': 1, 'ハ': 1}</span>及び<span>ハ{'ロ': 2}</span>に掲げる事項</Sentence>
