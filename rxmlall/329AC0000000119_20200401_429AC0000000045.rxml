          <Article Delete="false" Hide="false" Num="98">
            <ArticleCaption>（仮換地の指定）</ArticleCaption>
            <ArticleTitle>第九十八条</ArticleTitle>
            <Paragraph Hide="false" Num="1">
              <ParagraphNum/>
              <ParagraphSentence>
