                  <ItemTitle>九</ItemTitle>
                  <ItemSentence>
                    <Sentence WritingMode="vertical">仮<span>換地{'概要': 1}</span>の<span>指定{'した': 1}</span></Sentence>
                  </ItemSentence>
                </Item>
                <Item Delete="false" Hide="false" Num="10">
