          <Item Delete="false" Hide="false" Num="27">
            <ItemTitle>二十七</ItemTitle>
            <ItemSentence>
              <Sentence WritingMode="vertical">保険募集を行う者の教育を行う業務</Sentence>
            </ItemSentence>
