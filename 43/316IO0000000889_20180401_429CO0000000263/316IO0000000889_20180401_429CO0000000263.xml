        </ParagraphSentence>
      </Paragraph>
    </SupplProvision>
    <SupplProvision AmendLawId="348002" AmendLawNum="昭和四八年一月二二日政令第二号" Extract="true" Id="a55298c0-189d-11e8-a242-005056b30d52">
